.pageManage{
  padding: 10rpx 0;
  background: #f8f8f8;
}
.manage-member, .manage-member-title{
  width: 100%;
}
.manage-member-title{
  padding: 0 20rpx;
  box-sizing: border-box;
}
.manage-item{
  display: flex;
  text-align: left;
  width: 100%;
  overflow: hidden;
  background: #fff;
  margin: 10rpx 0;
  padding: 0 20rpx;
  box-sizing: border-box;
  min-height: 40px;
  line-height: 40px;
  font-size: 32rpx;
  position: relative;
}
.manage-item .manage-arrow{
  position: absolute;
  right: 10px;
  top: 50%;
  display: inline-block;
  content: "";
  width: 7px;
  height: 7px;
  border: solid #999;
  border-width: 1px 1px 0 0;
  transform: translate(0,-50%) rotate(45deg);
}
.manage-item .inline-right-text{
  position: absolute;
  right: 50rpx;
  top: 50%;
  font-size: 30rpx;
  transform: translateY(-50%);
}
.member-image{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}